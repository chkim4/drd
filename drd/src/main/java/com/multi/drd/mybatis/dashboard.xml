<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.drd.dashboard">
	<select id="read" resultType="member" parameterType="member">
		select * from member
		WHERE id = #{id}
	</select>
	<select id="readbio" resultType="memberBio" parameterType="String">
		select * from memberbio
		where memberbioseq = (select memberseq
							from member
							where id = #{id})
	</select>

</mapper>